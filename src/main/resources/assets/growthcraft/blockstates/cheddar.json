{
  "multipart": [
    {   "when": { "cheese_state_top": "unaged" },
      "apply": { "model": "growthcraft:block/cheddar_top" }
    },
    {   "when": { "cheese_state_bottom": "unaged" },
      "apply": { "model": "growthcraft:block/cheddar_bottom" }
    },
    {   "when": { "cheese_state_top": "waxed" },
      "apply": { "model": "growthcraft:block/waxed_cheddar_top" }
    },
    {   "when": { "cheese_state_bottom": "waxed" },
      "apply": { "model": "growthcraft:block/waxed_cheddar_bottom" }
    },
    {   "when": { "cheese_state_top": "aged" },
      "apply": { "model": "growthcraft:block/waxed_cheddar_top" }
    },
    {   "when": { "cheese_state_bottom": "aged" },
      "apply": { "model": "growthcraft:block/waxed_cheddar_bottom" }
    }
  ]
}